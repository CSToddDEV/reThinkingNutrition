<a href="/">
  <span id="menu_span">
    <p id='menu_logo'><img src='../images/logo.webp'></p>
  </span>
</a>

<h1>Records for {{record.[0].title}}</h1>
<div class="response">
  <table>
    <tr>
      {{#each record.[0].keys}}
        <th>
          {{this}}
        </th>
      {{/each}}
    <tr>
      {{#each record.[0].values}}
    <tr>
      {{#each value}}
        <th>
          {{this}}
        </th>
      {{/each}}
    <tr>
      {{/each}}
  </table>
</div>

